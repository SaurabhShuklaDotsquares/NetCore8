        {
            <p id="totalresults">Showing @Model.totalItem Results for Country: @ViewBag.v_destinationName, Duration: @ViewBag.v_duration Days, Budget: @ViewBag.v_budgetTitle</p>
        }
        else if (ViewBag.v_duration != null)
