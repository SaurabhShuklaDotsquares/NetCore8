.align-right {
    text-align: right;
    margin-top: 2%;
}

.content-header, .content {
    padding-top: 0px;
}

.field-validation-error {
    color: red;
}

.input-validation-error {
    border: solid 1px #ff0000;
}

.modal-backdrop {
    opacity: 0.5 !important;
}



.icheck-primary > input:first-child:checked + input[type="hidden"] + label::before, .icheck-primary > input:first-child:checked + label::before {
    background-color: #29333d;
    border-color: #29333d;
}

.login-card-body .icheck-primary label {
    font-weight: normal;
}

    .login-card-body .icheck-primary label:focus {
        outline: none;
    }

.login-card-body .input-group .input-group-text, .register-card-body .input-group .input-group-text {
    color: #29333d;
}

.forgot-pass a {
    color: #29333d;
    font-weight: 600;
}

.btn-submit {
    background-color: #E5422B;
    color: #ffffff;
    border: none;
}

    .btn-submit:hover {
        background-color: #dc331b;
    }

.login-card-body input, .login-card-body select.form-control {
    border-radius: 3px !important;
    border: 1px solid rgba(96, 125, 159, 0.25);
    border-right: 1px solid rgba(96, 125, 159, 0.25) !important;
    background-color: #fbfcfe;
    -webkit-box-shadow: inset 0 1px 4px -1px rgba(96, 125, 159, 0.25);
    box-shadow: inset 0 1px 4px -1px rgba(96, 125, 159, 0.25);
    width: 100% !important;
    padding-right: 40px;
}

    .login-card-body input:focus, .login-card-body select:focus {
        box-shadow: none;
        border: 1px solid rgba(96, 125, 159, 0.50);
    }

.login-card-body .input-group .input-group-append {
    position: absolute;
    right: 0px;
    top: 5px;
    border: none;
    z-index: 99;
}

    .login-card-body .input-group .input-group-append .input-group-text {
        border: none;
    }

.login-card-body .input-group {
    position: relative;
    display: inherit;
}

#Email-error, #Password-error, .required-erro {
    font-size: 12px;
}

.forgot-btns .btn {
    display: inline-block;
    vertical-align: top;
    border-radius: 0px;
}

.forgot-btns {
    margin: auto;
    text-align: center;
}

    .forgot-btns .btn.btn-gray {
        background-color: rgba(96, 125, 159, 0.35);
        color: #3f4d5a;
        margin-right: 10px;
    }

        .forgot-btns .btn.btn-gray:hover {
            background-color: rgba(96, 125, 159, 0.50);
        }

.forgot-pass {
    text-align: right;
    font-size: 14px;
    text-decoration: underline;
    margin-top: 10px;
}

span.status.Active {
    background-color: #27AB83;
    display: inline-block;
    width: 15px;
    height: 10px;
    border-radius: 100%;
    border: 1px solid transparent;
}

span.status.Disabled {
    background-color: #29333d;
    display: inline-block;
    width: 15px;
    height: 10px;
    border-radius: 100%;
    border: 1px solid transparent;
}

.hidden {
    display: none;
}

.add-minus-col.close-box a i {
    color: #c93636;
}

.add-minus-col.close-box {
    position: relative;
    /* right: -14px; */
    top: -6px;
}

.add-minus-col {
    color: #b9bdbd;
    float: right;
    font-size: 21px;
}

.basicimages-box .delete-photo {
    background-color: transparent;
    padding: 0px;
    width: 100% !important;
    max-width: 100%;
    overflow: unset !important;
    text-overflow: ellipsis;
    position: relative;
    font-size: 14px;
    padding-right: 20px;
    text-align: left;
    cursor: inherit;
}

.border-dark-shadow {
    box-shadow: 0 0 1px rgba(0, 0, 0, 0.125), 0 1px 3px rgb(51, 64, 77);
}

.btn-postion {
    position: relative;
}

    .btn-postion span {
        background-color: #33404d;
        border-radius: 50%;
        color: #fff;
        font-size: 10px;
        line-height: 20px;
        position: absolute;
        left: -8px;
        top: -8px;
        text-align: center;
        width: 20px;
    }

#content-container.card-col-right-block {
    width: 100%;
}
.calendar-field label {
    margin-top: 2px;
}

.calendar-field input.form-control {
    border:none;
    padding:0;
}
#main-content {
    margin: 0px auto;
}
.btn {
    border-radius: 6px;
}
.tab-conten-card-block .card-body {
    padding:0;
}
.tab-conten-card-block .card {
    box-shadow: none;
    border-radius: 0.5rem;
    overflow: hidden;
    border: solid 1px #a5b6c8;
}
.tab-conten-card-block .card-header {
    background-color: #a5b6c8 !important;
}
.tab-conten-card-block .card-header h3{
    margin:0;
}
.tab-conten-card-block .card  .card-body {
    padding: 20px;
}
.flexible-content-box.hotel-details-card-block {
    border: none;
    background: transparent;
    padding: 0;
}
.flexible-content-box.hotel-details-card-block .card-header {
    background-color: transparent !important;
    border: none;
    padding: 0;
    padding-top: 0px;
}
.flexible-content-box.hotel-details-card-block .card-header .card-title {
    display: block;
    font-size: 20px;
    margin-bottom: 15px;
}
.flexible-content-box.hotel-details-card-block .card-body.roomCard {
    padding: 0;
}
.imgListBlock {
    display: flex;
    flex-wrap: wrap;
    margin-bottom: 30px;
}
.imgListBlockCard {
    width: 15%;
    flex: 0 0 15%;
    margin-right: 15px;
    margin-bottom: 15px;
    border: solid 1px #a5b6c8;
    border-radius: 10px;
    overflow: hidden;
    height: 220px;
}
.imgListBlockCard img {
    width: 100%;
    height:100%;
    object-fit: cover;
}
.quality-box-set {
    border-radius:10px;
    box-shadow: none;
    overflow: hidden;
}
.quality-inner-box label.lable-box {
    font-size: 16px;
    display: flex;
    align-items: center;
    font-weight: 400 !important;
}
.form-control[readonly] {
    background-color: #fff;
    opacity: 1;
    border: none !important;
    padding: 0;
}
.profile-photo-section .app-upload-img .reapeat-upload-img {
    border: solid 1px #c8d3db;
}
.profile-photo-section .app-upload-img .reapeat-upload-img img, .profile-photo-section .app-upload-img .reapeat-upload-img video {
    object-fit: contain;
}
.tab-conten-card-block .card-tools .btn {
    background-color: #33404d;
}
.tab-conten-card-block .card-tools {
    margin-top: -4px;
}
.unit-box-card-block .card-tools {
    margin-top:0;
}
.unit-box-card-block .card-title {
    color:#fff;
}
.custom-checkbox-list-box {
    display: flex;
    align-content: center;
}
    .custom-checkbox-list-box .custom-checkbox {
        margin-bottom: 0;
        margin-top: 5px;
    }
.custom-checkbox-list-box .custom-control-label::before {
    top: 1px;
}
    .custom-checkbox-list-box .custom-control-label::after {
        top: 1px;
    }
.add-tag-block {
    display: flex;
    align-items: self-end;
    max-width: 470px;
    margin-bottom: 20px;
    justify-content: space-between;
}
.add-tag-block-left {
    width: 90px;
    flex: 0 0 90px;
    margin-left:10px;
}
.add-tag-block-right {
    flex: auto;
}
    .add-tag-block-right .form-group {
        margin-bottom:0;
    }
.tariff-block ul {
    flex-wrap: wrap;
    margin:20px 0 0 0;
    padding: 0;
}

.tariff-block ul li {text-align: center;margin-right: 20px;list-style:none;}

.tariff-block ul li:last-child {
    margin-right: 0;
}

.tariff-block ul li img {
    display: block;
    margin: 0 auto 6px;
}

.tariff-block ul li span {
    display: block;
    color: #4e4e4e;
    font-size: 14px;
}

.form-control.datecalender {
    border: 1px solid #E1E1E1 !important;
    padding: 0.375rem 0.75rem;
}
.pkgduration {
    font-weight: normal !important;
}
.form-control.datecalendertime {
    border: 1px solid #E1E1E1 !important;
    padding: 0.375rem 0.75rem;
}

.InclusioncheckList, .ExclusioncheckList {
    display: flex;
    align-items: center;
}
    .InclusioncheckList label, .ExclusioncheckList label {
        display: block;
        margin: 0 0 0 5px;
    }

@media only screen and (max-width: 1650px) {
    .custom-grid-table {
        display:inline-table;
    }
}


@media only screen and (max-width: 991px) {
    .crd-mrg {
        margin-top: 20px;
        max-width: 100%;
        flex: 0 0 100%;
    }
    .custom-grid-table {
        display:block;
    }
}
